<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>User Ads Data</title>
	<link href="https://cdn.jsdelivr.net/npm/tailwindcss@2.2.19/dist/tailwind.min.css" rel="stylesheet">
</head>
<body class="bg-gray-100 text-gray-900">
	<div class="container mx-auto p-4">
		<h1 class="text-2xl font-bold mb-4">User Ads Data</h1>
		<table class="min-w-full bg-white border border-gray-200">
			<thead>
				<tr>
					<th class="py-2 px-4 border-b">Account Name</th>
					<th class="py-2 px-4 border-b">Ad ID</th>
					<th class="py-2 px-4 border-b">Ad Name</th>
					<th class="py-2 px-4 border-b">Creative</th>
					<th class="py-2 px-4 border-b">Impressions</th>
					<th class="py-2 px-4 border-b">Clicks</th>
					<th class="py-2 px-4 border-b">Spend</th>
				</tr>
			</thead>
			<tbody>
				<% adsData.forEach(ad => { %>
					<tr>
						<td class="py-2 px-4 border-b"><%= ad.accountName %></td>
						<td class="py-2 px-4 border-b"><%= ad.adId %></td>
						<td class="py-2 px-4 border-b"><%= ad.adName %></td>
						<td class="py-2 px-4 border-b"><%= JSON.stringify(ad.creative) %></td>
						<td class="py-2 px-4 border-b"><%= ad.insights.impressions %></td>
						<td class="py-2 px-4 border-b"><%= ad.insights.clicks %></td>
						<td class="py-2 px-4 border-b"><%= ad.insights.spend %></td>
					</tr>
				<% }) %>
			</tbody>
		</table>
	</div>
</body>
</html>
